  <stage stageid="85534bd5-91a3-4f7b-99b4-12e429b07709" name="GetAllProcesses" type="Code">
    <subsheetid>cd82ed7e-5ac0-428a-954d-bec674506202</subsheetid>
    <loginhibit />
    <display x="15" y="-75" w="90" h="30" />
    <outputs>
      <output type="collection" name="Processes" stage="processes" />
    </outputs>
    <onsuccess>c4943fd6-6a26-48ce-a09e-662d54f9f80f</onsuccess>
    <code><![CDATA[Dim p As System.Diagnostics.Process = New System.Diagnostics.Process()
    p.StartInfo.UseShellExecute = False
    p.StartInfo.RedirectStandardOutput = True
    p.StartInfo.CreateNoWindow = True
    p.StartInfo.FileName = "cmd.exe"
    p.StartInfo.Arguments = "/c tasklist.exe /V /FO CSV"
    p.Start()
    Dim output As String = p.StandardOutput.ReadToEnd()
    p.WaitForExit()
    Dim ots As String() = System.Text.RegularExpressions.Regex.Split(output, Environment.NewLine)
    Processes = New System.Data.DataTable()
    Dim isFirst As Boolean = True

    For Each ot As String In ots
        If ot = "" Then Continue For
        Dim ot2 As String = ot.Substring(1, ot.Length - 3)
        Dim vals2 As String() = System.Text.RegularExpressions.Regex.Split(ot2, """,""")

        If isFirst Then
            isFirst = False

            For Each val As String In vals2
                Processes.Columns.Add(val)
            Next

            Continue For
        End If

        Processes.Rows.Add(vals2)
    Next]]></code>
  </stage>
