  <stage stageid="c577e2ac-7fc6-4f15-915c-dbb30798ecbd" name="Code1" type="Code">
    <subsheetid>034f9fb5-7745-42c2-9145-037f1b6f012c</subsheetid>
    <loginhibit />
    <display x="-420" y="0" />
    <inputs>
      <input type="text" name="vmName" expr="[vmName]" />
    </inputs>
    <outputs>
      <output type="collection" name="vmOSProperties" stage="vmOSProperties" />
    </outputs>
    <onsuccess>34acf008-0ca8-4ba2-bd5c-888f19816c5f</onsuccess>
    <code><![CDATA[  ConnectionOptions options = new ConnectionOptions();
       // options.Impersonation = System.Management.ImpersonationLevel.Impersonate;
DataRow dr;
vmOSProperties = new DataTable();
vmOSProperties.Columns.Add("Title");
vmOSProperties.Columns.Add("Value");
vmOSProperties.Columns.Add("Machine Name");
        try
		{
        ManagementScope scope = new ManagementScope(@"\\"+vmName+"\\root\\cimv2", options);
        scope.Connect();

        //Query system for Operating System information
        ObjectQuery query = new ObjectQuery("SELECT * FROM Win32_OperatingSystem");
        ManagementObjectSearcher searcher = new ManagementObjectSearcher(scope,query);

        ManagementObjectCollection queryCollection = searcher.Get();
      



  foreach ( ManagementObject m in queryCollection)
        {                   
			
			 
            
		      dr = vmOSProperties.NewRow();   
           dr["Title"] ="Computer Name";
		   dr["Value"] = m["csname"];
		   dr["Machine Name"] =vmName;
		    vmOSProperties.Rows.Add(dr);
		   
		   
		   
			 dr = vmOSProperties.NewRow();
			  
		    dr["Title"] ="Windows Directory";
		   dr["Value"] = m["WindowsDirectory"];
		    dr["Machine Name"] =vmName;
		   vmOSProperties.Rows.Add(dr);
		   
			 dr = vmOSProperties.NewRow();
			  
		    dr["Title"] ="Operating System";
		   dr["Value"] = m["Caption"];
		   dr["Machine Name"] =vmName;
		    vmOSProperties.Rows.Add(dr);
			
			
			 dr = vmOSProperties.NewRow();
			
		    dr["Title"] ="Version";
		   dr["Value"] =  m["Version"];
		      dr["Machine Name"] =vmName;
			  
			     vmOSProperties.Rows.Add(dr);
			
				 
				 
				  dr = vmOSProperties.NewRow();
			
		     dr["Title"] ="Manufacturer";
		   dr["Value"] =  m["Manufacturer"];
		    dr["Machine Name"] =vmName;
		   vmOSProperties.Rows.Add(dr);
		   
		     dr = vmOSProperties.NewRow();
			
		     dr["Title"] ="SerialNumber";
		   dr["Value"] =  m["SerialNumber"];
		    dr["Machine Name"] =vmName;
		   vmOSProperties.Rows.Add(dr);
		   
		 
           
  
}


		}
		catch
		{  dr = vmOSProperties.NewRow();
			
		     dr["Title"] ="Invalid VM/Don't have Access";
		    dr["Machine Name"] =vmName;
		   vmOSProperties.Rows.Add(dr);}]]></code>
  </stage>
