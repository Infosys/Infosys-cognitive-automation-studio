    <loginhibit />
    <display x="15" y="75" />
    <inputs>
      <input type="text" name="targetDirectory" expr="[sourcePath]" />
    </inputs>
    <outputs>
      <output type="text" name="OutputLatestFileName" stage="latestFile" />
    </outputs>
    <onsuccess>577aaf55-5441-48a3-bbb3-a604679869ac</onsuccess>
    <code><![CDATA[DateTime? dtLatestDateTime = null;
OutputLatestFileName = string.Empty;
foreach( var eachfile in Directory.EnumerateFiles(targetDirectory))
{
		var info= new FileInfo(eachfile);
		var fileUpadeteTime= File.GetLastWriteTime(eachfile);
			
			if(dtLatestDateTime==null)
			{
				OutputLatestFileName=info.Name;
			dtLatestDateTime=fileUpadeteTime;
			}
			
			if(dtLatestDateTime<fileUpadeteTime)
			{
				OutputLatestFileName=info.Name;
			   dtLatestDateTime=fileUpadeteTime;
			}
}
]]></code>
  </stage>
