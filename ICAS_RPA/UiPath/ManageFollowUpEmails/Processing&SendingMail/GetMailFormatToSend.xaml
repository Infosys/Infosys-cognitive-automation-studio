  <Sequence sap2010:Annotation.AnnotationText="Checks the Inbox to filter out emails for which response is received. Also, checks Sent Items and categorizes the emails to determine which format to send for follow up." DisplayName="GetOutlookMails" sap2010:WorkflowViewState.IdRef="Sequence_2">
    <Sequence.Variables>
      <Variable x:TypeArguments="x:Int32" Name="mailsToSend" />
      <Variable x:TypeArguments="x:String" Name="mailSubject" />
      <Variable x:TypeArguments="x:String" Name="responseReceivedStatus" />
    </Sequence.Variables>
    <Assign DisplayName="Get Mail subject to search in Inbox/SentItems" sap2010:WorkflowViewState.IdRef="Assign_27">
      <Assign.To>
        <OutArgument x:TypeArguments="x:String">[mailSubject]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="x:String">[in_Config("MailSubject").ToString]</InArgument>
      </Assign.Value>
    </Assign>
    <Assign DisplayName="Get Response Received Status from Config" sap2010:WorkflowViewState.IdRef="Assign_28">
      <Assign.To>
