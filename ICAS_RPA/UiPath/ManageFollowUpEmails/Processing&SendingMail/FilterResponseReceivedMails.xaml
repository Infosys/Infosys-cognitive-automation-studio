  <Sequence sap2010:Annotation.AnnotationText="Checks mails from Inbox and filters out Email Ids for which response has been received." DisplayName="FilterResponseReceivedMails" sap2010:WorkflowViewState.IdRef="Sequence_5">
    <Sequence.Variables>
      <Variable x:TypeArguments="scg:List(snm:MailMessage)" Name="mailMessagesReceived" />
      <Variable x:TypeArguments="x:String" Name="testDataString" />
      <Variable x:TypeArguments="x:Int32" Name="timeOut" />
    </Sequence.Variables>
    <Assign DisplayName="Get TimeOut for Outlook from Config" sap2010:WorkflowViewState.IdRef="Assign_3">
      <Assign.To>
        <OutArgument x:TypeArguments="x:Int32">[timeOut]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="x:Int32">[Convert.ToInt32((Convert.ToDouble(in_TimeOutToCheckMails)))]</InArgument>
      </Assign.Value>
    </Assign>
    <ui:GetOutlookMailMessages Account="{x:Null}" DisplayName="Get Outlook Inbox Mail Messages with the matching Subject from Config" Filter="[&quot;@SQL=&quot; + &quot;&quot;&quot;&quot; + &quot;urn:schemas:httpmail:subject&quot; + &quot;&quot;&quot;&quot;  + &quot; like '%&quot;+in_MailSubject+&quot;%'&quot;]" GetAttachements="False" sap2010:WorkflowViewState.IdRef="GetOutlookMailMessages_1" MailFolder="Inbox" MarkAsRead="False" Messages="[mailMessagesReceived]" OnlyUnreadMessages="False" TimeoutMS="[timeOut]" Top="-1" />
    <WriteLine DisplayName="Count of mails received with the matching Subject" sap2010:WorkflowViewState.IdRef="WriteLine_5" Text="[&quot;Mails Received With Matching Subject - Count: &quot;+mailMessagesReceived.Count.ToString]" />
    <If Condition="[mailMessagesReceived.Count&gt;=1]" DisplayName="Check for mails received with the same Subject and To Email Id and categorize as 'Response Received'" sap2010:WorkflowViewState.IdRef="If_2">
      <If.Then>
        <Sequence DisplayName="Categorize as 'Response Received' for mails received in the given list of emails in config" sap2010:WorkflowViewState.IdRef="Sequence_4">
          <ui:ForEachRow CurrentIndex="{x:Null}" DataTable="[in_out_ToDetailsDT]" DisplayName="For Each To List" sap2010:WorkflowViewState.IdRef="ForEachRow_1">
            <ui:ForEachRow.Body>
              <ActivityAction x:TypeArguments="sd:DataRow">
                <ActivityAction.Argument>
                  <DelegateInArgument x:TypeArguments="sd:DataRow" Name="row" />
                </ActivityAction.Argument>
                <Sequence DisplayName="Body" sap2010:WorkflowViewState.IdRef="Sequence_3">
                  <ui:ForEach x:TypeArguments="snm:MailMessage" CurrentIndex="{x:Null}" DisplayName="Check for emails with matching subject" sap2010:WorkflowViewState.IdRef="ForEach`1_1" Values="[mailMessagesReceived]">
                    <ui:ForEach.Body>
                      <ActivityAction x:TypeArguments="snm:MailMessage">
                        <ActivityAction.Argument>
                          <DelegateInArgument x:TypeArguments="snm:MailMessage" Name="mail" />
                        </ActivityAction.Argument>
                        <Sequence DisplayName="Body" sap2010:WorkflowViewState.IdRef="Sequence_2">
                          <If Condition="[mail.From.ToString.Trim.Contains(row(&quot;To_MailID&quot;).ToString.Trim)]" DisplayName="Check for matching Email Id" sap2010:WorkflowViewState.IdRef="If_1">
                            <If.Then>
                              <Assign DisplayName="Mark as 'Response Received'" sap2010:WorkflowViewState.IdRef="Assign_2">
                                <Assign.To>
                                  <OutArgument x:TypeArguments="x:Object">[row("Mail_Sent_Status")]</OutArgument>
                                </Assign.To>
                                <Assign.Value>
                                  <InArgument x:TypeArguments="x:Object">[String.Concat(in_ResponseReceivedStatus," ",DateTime.Now.ToString("yyyy-MM-dd HH:mm"))]</InArgument>
                                </Assign.Value>
                              </Assign>
                            </If.Then>
                          </If>
                        </Sequence>
                      </ActivityAction>
                    </ui:ForEach.Body>
                  </ui:ForEach>
                </Sequence>
              </ActivityAction>
            </ui:ForEachRow.Body>
          </ui:ForEachRow>
        </Sequence>
      </If.Then>
    </If>
    <ui:OutputDataTable DataTable="[in_out_ToDetailsDT]" DisplayName="Output Data Table - with Responses received ids" sap2010:WorkflowViewState.IdRef="OutputDataTable_3" Text="[testDataString]" />
    <WriteLine DisplayName="Write Data Table to Output/Console" sap2010:WorkflowViewState.IdRef="WriteLine_4" Text="[&quot;Response received mails: &quot;+testDataString.ToString]" />
    <sads:DebugSymbol.Symbol></sads:DebugSymbol.Symbol>
